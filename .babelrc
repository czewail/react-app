{
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": "usage",
      "debug": true,
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-syntax-dynamic-import",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      }
    ]
  ],
  "env": {
    "debug": {
      "sourceMap": "inline",
      "retainLines": true
    }
  }
}